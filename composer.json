{
    "name": "itkdev/adgangsstyring_drupal",
    "type": "drupal-module",
    "authors": [
        {
            "name": "Mikkel Ricky",
            "email": "rimi@aarhus.dk"
        }
    ],
    "repositories": {
        "itk-dev/adgangsstyring": {
            "type": "vcs",
            "url": "https://github.com/itk-dev/adgangsstyring"
        }
    },
    "require": {
      "itkdev/adgangsstyring": "dev-develop"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "mglaman/drupal-check": "^1.1"
    },
    "scripts": {
        "coding-standards-check/drupal-check": [
            "vendor/bin/drupal-check --deprecations --analysis --exclude-dir=vendor *.* src"
        ],
        "coding-standards-check/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "coding-standards-check": [
            "@coding-standards-check/phpcs",
            "@coding-standards-check/drupal-check"
        ],
        "coding-standards-apply/phpcs": [
            "vendor/bin/phpcbf --standard=phpcs.xml.dist"
        ],
        "coding-standards-apply": [
            "@coding-standards-apply/phpcs"
        ]
    }
}
